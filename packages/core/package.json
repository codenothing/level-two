  "devDependencies": {
    "@types/benchmark": "^2.1.2",
    "@types/jest": "^29.5.3",
    "@types/mysql": "^2.15.21",
    "@types/node": "^20.4.2",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "benchmark": "^2.1.4",
    "eslint": "^8.45.0",
    "jest": "^29.6.1",
    "mysql": "^2.18.1",
    "redis": "^4.6.7",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.6"
  },
