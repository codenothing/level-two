{
  "name": "@level-two/kafka",
  "version": "1.2.0",
  "description": "Kafka broadcast extension for LevelTwo",
  "author": "Corey Hart <corey@codenothing.com>",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git://github.com/codenothing/level-two.git"
  },
  "scripts": {
    "clean": "rm -rf dist",
    "build": "yarn clean && tsc -p tsconfig.dist.json",
    "lint": "eslint . --ext .ts",
    "pretest": "yarn build && yarn lint",
    "test": "jest --verbose --coverage",
    "e2e": "../../scripts/docker.sh && yarn e2e-pure",
    "e2e-pure": "jest --config=e2e/jest.config.js",
    "prepublish": "yarn test"
  },
  "keywords": [
    "cache",
    "memory",
    "util",
    "kafka",
    "level-two"
  ],
  "files": [
    "dist",
    "package.json",
    "README.md",
    "LICENSE"
  ],
  "devDependencies": {
    "@types/jest": "^29.5.3",
    "@types/node": "^20.4.2",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.45.0",
    "jest": "^29.6.1",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.6"
  },
  "dependencies": {
    "@level-two/core": "^1.2.0",
    "kafkajs": "^2.2.4"
  },
  "publishConfig": {
    "access": "public"
  }
}
